{
  "manifest_version": 3,
  "name": "Tab Manager",
  "description": "Manage and organize your tabs.",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
